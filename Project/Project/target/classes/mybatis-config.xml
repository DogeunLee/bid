<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
		<typeAlias type="com.bid.board.member.model.vo.Member"
			alias="member" />
		<typeAlias type="com.bid.board.category.model.vo.BigCategory"
			alias="bigCat"/>
			<typeAlias type="com.bid.board.category.model.vo.DetailCategory"
			alias="detailCat"/>
			<typeAlias type="com.bid.board.member.model.vo.Graduate"
			alias="graduate"/>
			<typeAlias type="com.bid.board.member.model.vo.Exp"
			alias="exp"/>
			<typeAlias type="com.bid.board.member.model.vo.Certi"
			alias="certi"/>
	</typeAliases>

	<mappers>
		<mapper resource="/mappers/member-mapper.xml" />
		<mapper resource="/mappers/bigCat-mapper.xml" />
		<mapper resource="/mappers/detailCat-mapper.xml" />
	</mappers>

</configuration>
